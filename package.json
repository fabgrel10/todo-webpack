{
  "name": "todo-webpack",
  "version": "1.0.0",
  "description": "App to list the things that you need to do. Built using ES6 and Webpack.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config configuration/webpack.dev.config.js --mode=development",
    "watch": "webpack --config configuration/webpack.dev.config.js --mode=development --watch",
    "bundle": "npm install && npm run watch",
    "dev": "webpack serve --config configuration/webpack.dev.config.js --mode=development",
    "production": "webpack --config configuration/webpack.prod.config.js --mode=production"
  },
  "keywords": [
    "webpack",
    "boilerplate"
  ],
  "author": "Fabricio Garcia",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.16.4",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "mini-css-extract-plugin": "^2.4.5",
    "prettier": "^2.5.0",
    "sass": "^1.43.5",
    "sass-lint": "^1.13.1",
    "sass-loader": "^12.3.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "target": "web",
  "dependencies": {
    "nanoid": "^3.1.30"
  }
}
